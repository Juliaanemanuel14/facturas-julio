# Azure Document Intelligence (Form Recognizer)
# Obtén estas credenciales desde Azure Portal
AZURE_ENDPOINT=https://tu-recurso.cognitiveservices.azure.com/
AZURE_KEY=tu_clave_azure_aqui

# Google Gemini AI
# Obtén la API key desde https://aistudio.google.com/app/apikey
GEMINI_API_KEY=tu_api_key_gemini_aqui
GEMINI_MODEL=gemini-2.5-pro

# Authentication
# Usuario para login (por defecto: admin)
AUTH_USERNAME=admin
# Hash de la contraseña (genera con: node -e "const bcrypt = require('bcryptjs'); bcrypt.hash('tu_contraseña', 10).then(hash => console.log(hash));")
AUTH_PASSWORD_HASH=tu_password_hash_aqui
